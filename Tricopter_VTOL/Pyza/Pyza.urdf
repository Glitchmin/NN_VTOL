<?xml version="1.0" ?>

<robot name="Pyza">

  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>

  <material name="blue">
    <color rgba="0.0 0.0 1.0 1.0"/>
  </material>

  <material name="red">
    <color rgba="1 0.0 1.0 1.0"/>
  </material>

  <material name="white">
    <color rgba="1 1 1 1.0"/>
  </material>

  <!-- Give a tiny positive-definite inertia to avoid degenerate root -->
  <link name="base_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.02"/>
      <inertia ixx="1e-08" ixy="0" ixz="0" iyy="1e-08" iyz="0" izz="1e-08"/>
    </inertial>
  </link>

  <link name="horizontal_tail_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.02"/>
      <!-- Box: 0.15 x 0.3 x 0.05 (x,y,z) -->
      <inertia ixx="2.917e-05" ixy="0" ixz="0" iyy="4.167e-05" iyz="0" izz="1.667e-05"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.15 0.3 0.05"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <box size="0.15 0.3 0.05"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>
  <joint name="horizontal_tail_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-.5 0 0"/>
    <parent link="base_link"/>
    <child link="horizontal_tail_link"/>
  </joint>

  <link name="vertical_tail_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.02"/>
      <!-- Box: 0.15 x 0.05 x 0.2 (x,y,z) -->
      <inertia ixx="3.083e-05" ixy="0" ixz="0" iyy="1.667e-05" iyz="0" izz="1.917e-05"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.15 0.05 0.2"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <box size="0.15 0.05 0.2"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>
  <joint name="vertical_tail_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-.5 0 0.1"/>
    <parent link="base_link"/>
    <child link="vertical_tail_link"/>
  </joint>

  <link name="main_wing_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.28"/>
      <inertia ixx="0.02829" ixy="0" ixz="0" iyy="0.000992" iyz="0" izz="0.02917"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.2 1.1 0.05"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <box size="0.2 1.1 0.05"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>
  <joint name="main_wing_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 .01"/>
    <parent link="base_link"/>
    <child link="main_wing_link"/>
  </joint>

  <link name="fuselage_link3">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.25"/>
      <!-- Box: 0.12 x 0.05 x 0.05 -->
      <inertia ixx="5.208e-05" ixy="0" ixz="0" iyy="1.563e-04" iyz="0" izz="1.563e-04"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size=".12 .05 .05"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <box size=".12 .05 .05"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="fuselage_joint3" type="fixed">
    <origin rpy="0 0 0" xyz=".25 0 -0.02"/>
    <parent link="base_link"/>
    <child link="fuselage_link3"/>
  </joint>

  <link name="fuselage_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.02"/>
      <!-- Box: 0.8 x 0.01 x 0.01 -->
      <inertia ixx="1.667e-07" ixy="0" ixz="0" iyy="1.067e-05" iyz="0" izz="1.067e-05"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size=".8 0.01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <box size=".8 0.01 0.01"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="fuselage_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.15 0 0"/>
    <parent link="base_link"/>
    <child link="fuselage_link"/>
  </joint>

  <link name="motor3_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.1"/>
      <!-- Box: 0.05 x 0.05 x 0.02 -->
      <inertia ixx="4.583e-05" ixy="0" ixz="0" iyy="4.583e-05" iyz="0" izz="8.333e-06"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.05 0.05 0.02"/>
      </geometry>
      <material name="red"/>
    </visual>
  </link>
  <joint name="motor3_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.19 0 0"/>
    <parent link="base_link"/>
    <child link="motor3_link"/>
  </joint>

  <link name="ail_left_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.02"/>
      <!-- Using collision box: 0.15 x 0.5 x 0.02 -->
      <inertia ixx="4.416e-05" ixy="0" ixz="0" iyy="1.667e-05" iyz="0" izz="4.833e-05"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.15 0.5 0.02"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <box size="0.05 0.5 0.02"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>
  <joint name="ail_left_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.125 0.3 0"/>
    <parent link="base_link"/>
    <child link="ail_left_link"/>
  </joint>

  <link name="ail_right_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.02"/>
      <!-- Same collision box: 0.15 x 0.5 x 0.02 -->
      <inertia ixx="4.416e-05" ixy="0" ixz="0" iyy="1.667e-05" iyz="0" izz="4.833e-05"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.15 0.5 0.02"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <box size="0.05 0.5 0.02"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>
  <joint name="ail_right_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.125 -0.3 0"/>
    <parent link="base_link"/>
    <child link="ail_right_link"/>
  </joint>

  <link name="fuselage1_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.02"/>
      <!-- Box: 0.2 x 0.01 x 0.01 -->
      <inertia ixx="2.917e-08" ixy="0" ixz="0" iyy="3.417e-07" iyz="0" izz="3.417e-07"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.2 .01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <box size="0.2 .01 0.01"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="fuselage_joint1" type="fixed">
    <origin rpy="0 0 0" xyz=".1 .23 .01"/>
    <parent link="base_link"/>
    <child link="fuselage1_link"/>
  </joint>

  <link name="motor1_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.06"/>
      <!-- Box: 0.05 x 0.05 x 0.02 -->
      <inertia ixx="2.750e-05" ixy="0" ixz="0" iyy="2.750e-05" iyz="0" izz="5.000e-06"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.05 0.05 0.02"/>
      </geometry>
      <material name="red"/>
    </visual>
  </link>
  <joint name="motor1_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.22 .23 .01"/>
    <parent link="base_link"/>
    <child link="motor1_link"/>
  </joint>

  <link name="fuselage2_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.02"/>
      <!-- Box: 0.2 x 0.01 x 0.01 -->
      <inertia ixx="2.917e-08" ixy="0" ixz="0" iyy="3.417e-07" iyz="0" izz="3.417e-07"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.2 .01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <box size="0.2 .01 0.01"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>
  <joint name="fuselage_joint2" type="fixed">
    <origin rpy="0 0 0" xyz="0.1 -.23 .01"/>
    <parent link="base_link"/>
    <child link="fuselage2_link"/>
  </joint>

  <link name="motor2_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.06"/>
      <!-- Box: 0.05 x 0.05 x 0.02 -->
      <inertia ixx="2.750e-05" ixy="0" ixz="0" iyy="2.750e-05" iyz="0" izz="5.000e-06"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.05 0.05 0.02"/>
      </geometry>
      <material name="red"/>
    </visual>
  </link>
  <joint name="motor2_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.22 -.23 .01"/>
    <parent link="base_link"/>
    <child link="motor2_link"/>
  </joint>

</robot>
